<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('back_style') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('back_script') }}
        {% endblock %}
    </head>
    <body>

    <div class="container-fluid">
        <div class="row flex-nowrap">
            <div class="col-auto col-md-3 col-xl-2 px-sm-2 px-0 admin-menu ">
                {% include 'back/partials/menu.html.twig' %}
            </div>
            <div class="col py-3">
                {% block body %}
                {% endblock %}
            </div>
        </div>
    </div>

    </body>
</html>
